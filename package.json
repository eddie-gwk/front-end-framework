      "useEmoji": true
    }
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/eslint__js": "^8.42.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.2",
    "eslint": "~9.4.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "prettier": "3.3.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
